@model IEnumerable<COVID_19_Vaccination_System.Models.VaccinationsNewsModel>

@{
    foreach (var v in Model)
    {
        <div class="panel panel-default">
            <div class="panel-heading">@v.Day.ToShortDateString()</div>
            <div class="panel-body">На ден @v.Day.ToShortDateString() беа вакцинирани: @v.Vaccinations граѓани.</div>
        </div>
    }
}
