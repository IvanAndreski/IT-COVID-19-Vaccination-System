@model IEnumerable<COVID_19_Vaccination_System.Models.AppointmentModel>

@{
    ViewBag.Title = "Денешни термини";
}

<h2>@ViewBag.Title</h2>

@if (!Model.Any())
{
    <h3 class="text-">
        Нема закажани термини за ден @DateTime.Now.ToShortDateString()
    </h3>
}
else
{
    @Html.Partial("_AppointmentsOfUserPartial", Model)
}

<div class="btn btn-info">
    @Html.ActionLink("Дома", "Index", "Home", null, new { style = "color: white; text-decoration:none;" })
</div>

